<!DOCTYPE html>
<html lang="en">
<head>
    <title>My Blog</title>
</head>
<body>
    <h1>Blog Posts</h1>
    <a href="/new">Create New Post</a>
    <ul>
        <% posts.forEach(post => { %>
            <li><a href="/post/<%= post._id %>"><%= post.title %></a></li>
        <% }) %>
    </ul>
</body>
</html>

